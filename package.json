  "description": "Listofacil com Nuxt",
  "author": "Leo Vilela/Manoel Neto/Carlos Eduardo Pedroni ",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && nuxt build --spa",
    "build:dev": "rm -rf dist && NODE_ENV=development nuxt build --spa",
    "build:stage": "rm -rf dist && NODE_ENV=stage nuxt build --spa",
    "build:pp": "rm -rf dist && NODE_ENV=preprod nuxt build --spa",
    "dev": "cross-env HOST=localhost PORT=3000 NODE_ENV=development nuxt",
    "stage": "cross-env HOST=localhost PORT=3000 NODE_ENV=stage nuxt",
    "start": "node src/server/prod.js",
    "start:preprod": "node src/server/preprod.js",
    "start:dev": "node src/server/dev.js --watch server",
    "start:homol": "cross-env NODE_ENV=stage node src/server/homol.js --watch server",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue .",
    "lint-fix": "eslint --ext .js,.vue . --fix",
    "precommit": "npm run lint",
    "local": "node src/server/local.js --watch server",
    "test": "jest",
    "test:unit": "jest --verbose --testMatch=**/*.spec.js",
    "test:unit:watch": "jest --watch --testMatch=**/*.spec.js",
    "audit": "npm audit",
    "serve:mock": "json-server --watch __mock__/db.json -p 3001"
  },
